{
  "name": "sui-web-components",
  "version": "0.0.1",
  "description": "Web Components with Utility Props for styling",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sui-web-components/sui-web-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "build-storybook": "build-storybook -s ./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@dcylabs/stencil-custom-elements-json": "^1.0.0",
    "@stencil/core": "^1.8.4",
    "@stencil/sass": "^1.1.1",
    "@storybook/addon-a11y": "^5.3.13",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/web-components": "^5.3.13",
    "babel-loader": "^8.0.6",
    "lit-html": "^1.1.2",
    "storybook-addon-web-components-knobs": "^0.2.8"
  },
  "license": "MIT",
  "dependencies": {
    "utility-props": "^0.1.6"
  }
}
